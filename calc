num1, operation, num2 = input('Enter calculation: ').split()
num1 = int(num1)
num2 = int(num2)
if operation == '+':
    print('Resalt:{}+{}={}'.format(num1, num2, num1+num2))
elif operation == '*':
    print('Resalt:{}*{}={}'.format(num1, num2, num1 * num2))
elif operation == '-':
    print('Resalt:{}-{}={}'.format(num1, num2, num1 - num2))
elif operation == '/':
    print('Resalt:{}/{}={}'.format(num1, num2, num1 / num2))
else:
    print('Incorrect operation')
